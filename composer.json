{
    "name": "colbydude/ffxiv-progression-checker",
    "description": "Simple interface for checking a character's raid progression for FFXIV.",
    "homepage": "http://ffxiv.voidteam.net/progression",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Colby Terry",
            "email": "colbydude@voidteam.net",
            "homepage": "https://colbydude.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Colbydude/xivapi-php.git"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "xivapi/xivapi-php": "dev-master"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
